<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema path="/com/github/slawtul/hamster/" id="com.github.slawtul.hamster"
            gettext-domain="com.github.slawtul.hamster">
        <key name="first-run" type="b">
            <default>true</default>
        </key>
        <key name="run-automatically" type="b">
            <default>true</default>
        </key>
        <key name="run-minimize" type="b">
            <default>false</default>
        </key>
        <key name="set-focus-on-search-input" type="b">
            <default>false</default>
        </key>
        <key name="clear-search-input" type="b">
            <default>false</default>
        </key>
        <key name="delay-pasting" type="d">
            <default>320</default>
        </key>
        <key name="eliminate-spaces" type="b">
            <default>true</default>
        </key>
        <key name="save-list" type="b">
            <default>true</default>
        </key>
        <key name="item-list-size" type="d">
            <default>256</default>
        </key>
        <key name="item-prefix" type="s">
            <default>""</default>
        </key>
        <key name="item-suffix" type="s">
            <default>"\\n"</default>
        </key>
        <key name="window-x" type="i">
            <default>-1</default>
        </key>
        <key name="window-y" type="i">
            <default>-1</default>
        </key>
    </schema>
</schemalist>
